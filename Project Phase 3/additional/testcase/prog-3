int main(int x, bool b) {
  int arr1[8];
  int arr2[8];
  int i;
  int j;

  if (x < 0) { return 0; }

  if (x >= 8) { return 0; }

  i = 0;
  j = i;

  while (i < 8) {
    arr1[i] = i * 3 + 1;
    arr2[i] = i * 3 + 2;
    i = i + 1;
  }

  j = i;
  if (b) {
    return arr1[x];
  } else {
    return arr2[x];
  }
}
